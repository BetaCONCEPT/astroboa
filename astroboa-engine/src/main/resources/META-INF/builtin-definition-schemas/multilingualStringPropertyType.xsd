<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	targetNamespace="http://www.betaconceptframework.org/schema/astroboa/utilTypes/multilingualStringPropertyType"
	xmlns:tns="http://www.betaconceptframework.org/schema/astroboa/model/utilTypes/multilingualStringPropertyType"
	xmlns:bccmsmodel="http://www.betaconceptframework.org/schema/astroboa/model"
	version="${project.version}">

	<xs:import namespace="http://www.betaconceptframework.org/schema/astroboa/model"
		schemaLocation="astroboa-model-${project.version}.xsd" />

	<xs:complexType name="multilingualStringPropertyType">
		<xs:annotation>
			<xs:documentation xml:lang="en">
				<bccmsmodel:displayName>MultiLingual String	Property</bccmsmodel:displayName>
								<bccmsmodel:description>This complex type represents a multilingual string property type</bccmsmodel:description>
			</xs:documentation>
			<xs:documentation xml:lang="el">
				Πολυγλωσσικό αλφαριθμητικό
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="bccmsmodel:complexCmsPropertyType">
				<xs:sequence>

					<xs:element name="en" minOccurs="0" maxOccurs="1"
						type="xs:string">
						<xs:annotation>
							<xs:documentation xml:lang="en">
								<bccmsmodel:displayName>English</bccmsmodel:displayName>
								<bccmsmodel:description>English Language</bccmsmodel:description>
								</xs:documentation>
							<xs:documentation xml:lang="el">
								Αγγλικά
								</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="sq" minOccurs="0" maxOccurs="1"
						type="xs:string">
						<xs:annotation>
							<xs:documentation xml:lang="el">
								Albanian
							</xs:documentation>
							<xs:documentation xml:lang="el">
								Αλβανικά
							</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="bg" minOccurs="0" maxOccurs="1"
						type="xs:string">
						<xs:annotation>
							<xs:documentation xml:lang="el">
								Bulgarian
							</xs:documentation>
							<xs:documentation xml:lang="el">
								Βουλγαρικά
							</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="fr" minOccurs="0" maxOccurs="1"
						type="xs:string">
						<xs:annotation>
							<xs:documentation xml:lang="en">
								<bccmsmodel:displayName>French</bccmsmodel:displayName>
								<bccmsmodel:description>French Language</bccmsmodel:description>
							</xs:documentation>
							<xs:documentation xml:lang="el">
								Γαλλικά
							</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="de" minOccurs="0" maxOccurs="1"
						type="xs:string">
						<xs:annotation>
							<xs:documentation xml:lang="el">
								German
							</xs:documentation>
							<xs:documentation xml:lang="el">
								Γερμανικά
							</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="el" minOccurs="0" maxOccurs="1"
						type="xs:string">
						<xs:annotation>
							<xs:documentation xml:lang="en">
								Greek
							</xs:documentation>
							<xs:documentation xml:lang="el">
								Ελληνικά
							</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="es" minOccurs="0" maxOccurs="1"
						type="xs:string">
						<xs:annotation>
							<xs:documentation xml:lang="el">
								Spanish
							</xs:documentation>
							<xs:documentation xml:lang="el">
								Ισπανικά
							</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="it" minOccurs="0" maxOccurs="1"
						type="xs:string">
						<xs:annotation>
							<xs:documentation xml:lang="el">
								Italian
							</xs:documentation>
							<xs:documentation xml:lang="el">
								Ιταλικά
							</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="nl" minOccurs="0" maxOccurs="1"
						type="xs:string">
						<xs:annotation>
							<xs:documentation xml:lang="el">
								Dutch
							</xs:documentation>
							<xs:documentation xml:lang="el">
								Ολλανδικά
							</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="pt" minOccurs="0" maxOccurs="1"
						type="xs:string">
						<xs:annotation>
							<xs:documentation xml:lang="el">
								Portuguese
							</xs:documentation>
							<xs:documentation xml:lang="el">
								Πορτογαλλικά
							</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="ro" minOccurs="0" maxOccurs="1"
						type="xs:string">
						<xs:annotation>
							<xs:documentation xml:lang="el">
								Romanian
							</xs:documentation>
							<xs:documentation xml:lang="el">
								Ρουμανικά
							</xs:documentation>
						</xs:annotation>
					</xs:element>

					<xs:element name="ru" minOccurs="0" maxOccurs="1"
						type="xs:string">
						<xs:annotation>
							<xs:documentation xml:lang="el">
								Russian
							</xs:documentation>
							<xs:documentation xml:lang="el">
								Ρώσικα
							</xs:documentation>
						</xs:annotation>
					</xs:element>

				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
</xs:schema>
